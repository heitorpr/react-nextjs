# Backend Environment Variables
# All variables use the APP_ prefix as defined in settings.py

# Database Configuration
# For Docker Compose: use 'db' as host (service name)
# For Local Development: use 'localhost' as host
APP_DB_USER=postgres
APP_DB_NAME=app
APP_DB_HOST=db
APP_DB_PORT=5432
APP_DB_PASSWORD=password
APP_DB_POOL_SIZE=10

# API Configuration
APP_TIMESTAMP_SIGNING_THRESHOLD=120000
APP_SECRET_KEY=your-secret-key-here

# Application Configuration
APP_DEBUG=true
APP_NAME=python_template
APP_NAMESPACE=personal
